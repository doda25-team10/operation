{
  "schemaVersion": 39,
  "title": "MyApp Metrics",
  "version": 1,
  "tags": [
    "myapp",
    "team-b"
  ],
  "panels": [
    {
      "type": "timeseries",
      "title": "Request Rate",
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=\"myapp\"}[5m]))",
          "legendFormat": "All"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "Error Rate (5xx)",
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=\"myapp\",status=~\"5..\"}[5m]))",
          "legendFormat": "5xx"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "Latency p95",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{job=\"myapp\"}[5m])) by (le))",
          "legendFormat": "p95"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 24,
        "h": 8
      }
    }
  ],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-6h",
    "to": "now"
  }
}

